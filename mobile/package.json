{
  "name": "treeinspector-mobile",
  "version": "1.0.0",
  "description": "TreeInspector Mobile App - Aplicativo móvel para inspeção de árvores",
  "main": "index.js",
  "scripts": {
    "android": "npx react-native run-android --no-packager",
    "start:metro": "npx react-native start",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace TreeInspectorMobile.xcworkspace -scheme TreeInspectorMobile -configuration Release -destination generic/platform=iOS -archivePath TreeInspectorMobile.xcarchive archive",
    "clean": "react-native clean-project-auto",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "cd ios && xcodebuild clean",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.2",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "react-native-screens": "^3.29.0",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-reanimated": "^3.6.2",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "react-native-vision-camera": "^3.6.17",
    "react-native-image-picker": "^7.1.0",
    "react-native-image-resizer": "^1.4.5",
    "@react-native-community/geolocation": "^3.2.1",
    "react-native-permissions": "^4.1.5",
    "react-native-maps": "^1.10.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-paper": "^5.12.3",
    "react-native-elements": "^3.4.3",
    "react-hook-form": "^7.48.2",
    "@hookform/resolvers": "^3.3.2",
    "yup": "^1.4.0",
    "axios": "^1.6.5",
    "react-query": "^3.39.3",
    "zustand": "^4.4.7",
    "immer": "^10.0.3",
    "date-fns": "^2.30.0",
    "react-native-date-picker": "^4.3.3",
    "react-native-modal": "^13.0.1",
    "react-native-toast-message": "^2.2.0",
    "react-native-loading-spinner-overlay": "^3.0.1",
    "react-native-progress": "^5.0.1",
    "react-native-fs": "^2.20.0",
    "react-native-share": "^10.0.2",
    "react-native-print": "^0.8.0",
    "react-native-pdf": "^6.7.3",
    "react-native-device-info": "^10.13.0",
    "react-native-network-info": "^5.2.1",
    "react-native-keychain": "^8.2.0",
    "react-native-biometrics": "^3.0.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-orientation-locker": "^1.7.0",
    "react-native-keep-awake": "^4.0.0",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/netinfo": "^11.2.1",
    "react-native-uuid": "^2.0.2",
    "react-native-qrcode-scanner": "^1.5.5",
    "react-native-svg": "^13.14.0"    
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.3.1",
    "typescript": "4.8.4",
    "detox": "^20.13.5",
    "@types/jest": "^29.5.8",
    "flipper-plugin-react-query-native-devtools": "^4.0.0"
  },
  "engines": {
    "node": ">=22.17.0",
    "npm": ">=10.0.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "e2e/config.json",
    "configurations": {
      "android.emu.debug": {
        "type": "android.emulator",
        "device": "Pixel_3a_API_30_x86"
      },
      "ios.sim.debug": {
        "type": "ios.simulator",
        "device": "iPhone 14"
      }
    }
  }
}